{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "umlib_base-theme": {
            "type": "package",
            "package": {
                "name": "mlibrary/umlib_base",
                "version": "1.0",
                "type": "drupal-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/mlibrary/umlib_base/archive/refs/heads/main.zip",
                    "reference": "main"
                }
            }
        },
        "umlib_admin-theme": {
            "type": "package",
            "package": {
                "name": "mlibrary/umlib_admin",
                "version": "1.0",
                "type": "drupal-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/mlibrary/umlib_admin/archive/refs/heads/main.zip",
                    "reference": "main"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/lti_tool_provider",
                "drupal/extra_field_description"
            ]
        },
        "drupal/lti_tool_provider": {
          "type": "package",
          "package": {
            "name": "drupal/lti_tool_provider",
            "version": "dev-custom",
            "type": "drupal-module",
            "source": {
              "type": "git",
              "url": "https://github.com/mlibrary/lti_tool_provider.git",
              "reference": "main"
            }
          }
        },
        "drupal/extra_field_description": {
          "type": "package",
          "package": {
            "name": "drupal/extra_field_description",
            "version": "dev-custom",
            "type": "drupal-module",
            "source": {
              "type": "git",
              "url": "https://github.com/mlibrary/extra_field_description.git",
              "reference": "main"
            }
          }
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/allow_iframed_site": "^3.0.3",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/autocomplete_field_match": "1.x-dev",
        "drupal/bartik": "^1.0",
        "drupal/block_classes": "^1.0",
        "drupal/ckeditor": "^1.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/cookie_samesite_support": "^1.0",
        "drupal/core": "^10.1",
        "drupal/devel": "^5.0",
        "drupal/disable_messages": "^2.0",
        "drupal/extra_field_description": "dev-custom",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.0",
        "drupal/field_show_all": "1.0.x-dev",
        "drupal/fieldblock": "^2.2",
        "drupal/fitvids": "^2.0",
        "drupal/force_users_logout": "^2.0.1",
        "drupal/iframe": "^2.23",
        "drupal/key": "^1.17",
        "drupal/lti_tool_provider": "dev-custom",
        "drupal/mailsystem": "^4.4",
        "drupal/node_view_permissions": "^1.6",
        "drupal/oembed_providers": "^2.1",
        "drupal/paragraphs": "^1.16",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/paragraphs_type_help": "^3.2",
        "drupal/rabbit_hole": "^2.0@alpha",
        "drupal/smtp": "^1.2",
        "drupal/swiftmailer": "^2.4",
        "drupal/term_csv_export_import": "^4.0",
        "drupal/token": "^1.13",
        "drupal/upgrade_status": "^4.0",
        "drush/drush": "^11.5",
        "oat-sa/lib-lti1p3-core": "^6.9",
        "oat-sa/lib-lti1p3-deep-linking": "^4.0",
        "mlibrary/umlib_admin": "^1.0",
        "mlibrary/umlib_base": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
          "drupal/core": {
            "https://www.drupal.org/node/2916876": "https://www.drupal.org/files/issues/2020-06-23/2916876-92.patch"
          },
          "drupal/token": {
            "https://www.drupal.org/node/919760": "https://www.drupal.org/files/issues/2020-02-10/token--current_page_object_token--919760--81.patch"
          },
          "drupal/cookie_samesite_support": {
            "https://www.drupal.org/node/3319614": "https://www.drupal.org/files/issues/2022-11-08/3319614-php-8.1-compatibility-2.patch"
          },
          "oat-sa/lib-lti1p3-core": {
            "dont use deployment id for validation": "patches/dont-use-deployment-id.patch"
          },
          "drupal/beautytips": {
            "put all beautytips on top-note that UI may allow soon": "patches/put-all-beautytips-on-top.patch",
            "change default from title to data-title to accomodate layout_builder_component_attributes-note that UI may allow soon": "patches/use-data-title-in-beautytips.patch"
          }
        }
    }
}
