<?php

/**
 * @file
 * Contains http_client_error_status.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function http_client_error_status_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the http_client_error_status module.
    case 'help.page.http_client_error_status':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provide HTTP client error status context plugin') . '</p>';
      return $output;

    default:
  }
}
